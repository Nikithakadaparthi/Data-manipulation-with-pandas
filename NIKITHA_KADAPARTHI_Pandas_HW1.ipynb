      "source": [
        "def Statusfun(x):\n",
        "    if x >= 3.5:\n",
        "        return \"Honor Roll\"\n",
        "    elif x < 3.5 and x >= 3:\n",
        "        return \"Normal\"\n",
        "    elif x < 3:\n",
        "        return \"In Probation\"\n",
        "\n",
        "#apply method applies a function to each element of a DataFrame\n",
        "df_students[\"Status\"] = df_students[\"GPA\"].apply(Statusfun)\n",
        "\n",
        "df_students"
      ]
    },
