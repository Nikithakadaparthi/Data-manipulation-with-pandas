        "def grade(x):\n",
        "    if x >= 90:\n",
        "        return 'A'\n",
        "    elif x < 90 and x >= 80:\n",
        "        return 'B'\n",
        "    elif x < 80 and x >= 70:\n",
        "        return 'C'\n",
        "    elif x < 70 and x >= 60:\n",
        "        return 'D'\n",
        "    elif x < 60:\n",
        "        return 'F'\n",
        "\n",
        "#apply method applies a function to each element of a DataFrame\n",
        "df_students[\"CS 5000 Grade\"] = df_students[\"CS 5000\"].apply(lambda x:grade(x))\n",
        "df_students"
      ]
    },
