        "def gpa(x):\n",
        "    if (x >= 90):\n",
        "        return 4.0\n",
        "    if (x >= 80) & (x < 90):\n",
        "        return 3.0\n",
        "    if (x >= 70) & (x < 80):\n",
        "        return 2.0\n",
        "    if (x >= 60) & (x < 70):\n",
        "        return 1.0\n",
        "    return 0.0\n",
        "\n",
        "#apply method applies a function to each element of a DataFrame\n",
        "df_students['GPA'] = df_students.apply(lambda x: (((gpa(x['CS 5000']) * 3) +\n",
        "                                                   (gpa(x['CS 5110']) * 3) +\n",
        "                                                   (gpa(x['CS 5600']) * 3)) / 9), axis = 1)\n",
        "df_students"
      ]
    },
